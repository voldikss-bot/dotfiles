{
  "coc.preferences.formatOnSaveFiletypes": [],
  "coc.preferences.messageLevel": "more",
  "coc.source.vimtex.priority": 99,
  "coc.source.file.priority": 10,
  "coc.source.around.priority": 8,
  "coc.source.tag.priority": 5,
  "yank.enableCompletion": false,
  "yank.priority": 4,
  "coc.browser.priority": 3,
  "coc.source.emoji.priority": 3,
  "coc.source.dictionary.priority": 1,
  "coc.source.buffer.priority": 1,
  "coc.source.word.priority": 0,
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.source.emoji.shortcut": "E",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "➤",
  "diagnostic-languageserver.filetypes": {
    "markdown": ["write-good"],
    "sh": "shellcheck"
  },
  "eslint.enable": true,
  "eslint.filetypes": ["javascript", "javascript.jsx", "javascriptreact"],
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "git.enableGutters": true,
  "git.addedSign.hlGroup": "GitAdd",
  "git.removedSign.hlGroup": "GitDeleteButtom",
  "git.topRemovedSign.hlGroup": "GitDeleteTop",
  "git.changeRemovedSign.hlGroup": "GitDeleteTopAndBottom",
  "git.changedSign.hlGroup": "GitModify",
  "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
  "prettier.disableLanguages": ["typescript"],
  "prettier.formatterPriority": 1,
  "prettier.printWidth": 85,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "python.autoComplete.addBrackets": true,
  "session.directory": "/home/voldikss/.cache/nvim/session",
  "smartf.timeout": 800,
  "smartf.jumpOnTrigger": true,
  "suggest.noselect": false,
  "suggest.numberSelect": false,
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.languageSourcePriority": 99,
  "suggest.completionItemKindLabels": {
    "function": "\u0192",
    "method": "\uf09a",
    "variable": "\ue79b",
    "constant": "\uf8fe",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "file": "\uf723",
    "value": "\uf89f",
    "event": "\ufacd",
    "folder": "\uf114",
    "keyword": "\uf1de",
    "snippet": "\ue60b",
    "operator": "\u03a8",
    "reference": "\ufa46",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "snippets.ultisnips.directories": [
    "snippets",
    "UltiSnips",
    "javascript",
    "coffee"
  ],
  "snippets.priority": -10,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "template.templatesDirectory": "~/.vim/templates",
  "todolist.autoUpload": true,
  "todolist.monitor": true,
  "tslint.autoFixOnSave": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.preferences.noSemicolons": true,
  "typescript.suggestionActions.enabled": false,
  "yank.highlight.enable": true,
  "yank.highlight.duration": 20,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "format": "b",
          "directory": "/home/voldikss/.cache/nvim/ccls-cache"
        },
        "capabilities": {
          "foldingRangeProvider": false,
          "workspace": {
            "workspaceFolders": {
              "supported": false
            }
          }
        },
        "clang": {
          "extraArgs": ["--gcc-toolchain=/usr"]
        },
        "completion": {
          "detailedLabel": true
        },
        "index": {
          "onChange": false,
          "initialNoLinkage": true,
          "threads": 2
        }
      }
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh", "zsh"],
      "ignoredRootPaths": ["~"]
    }
  }
  // vim:ft=jsonc
}
